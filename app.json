{
  "expo": {
    "name": "FoodMood",
    "slug": "food-mood",
    "version": "1.0.0",
    "splash": {
      "image": "./src/assets/images/splash-screen.png",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": ["./src/assets/images/*"],
    "orientation": "portrait",
    "scheme": "foodmood",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.foodmood"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.foodmood"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },

    "experiments": {
      "typedRoutes": true
    }
  }
}
